#include <stdio.h>
#include <stdlib.h>

int main ()
{
    int dizi[10], max, i;

    for (i=0; i<10; i++)
    {
        printf("Sayiyi Giriniz:%d\n", i+1);
        scanf("%d", &dizi[i]);
    }
    
    max = dizi[0];
    for(i=1; i<10; i++)
    {
        if (dizi[i] > max)
        {
            max = dizi[i];
        }
    }
    printf("En Buyuk Deger:%d\n", max);
    return 0;
}