#include <stdio.h>
#include <stdlib.h>
 
int oddNumbers()
{
    int sayi;


    printf("Bir Sayi Giriniz:\n");
    scanf("%d", &sayi);


    for (int i = 0; i <=sayi; i++)
    {
        
        if(i %2 == 1)
        {
            printf("%d", i);
        }
        printf(" ") ;   
    }
}


int main()
{
    oddNumbers();
    
    return 0;
}